    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-bearer-token": "^2.4.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.0.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "per-env": "^1.0.2",
    "pg": "^8.3.0",
    "react": "^16.14.0",
